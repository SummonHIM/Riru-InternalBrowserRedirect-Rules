{
  "tag": "QQ",
  "authors": "Kr328",
  "rules": [
    {
      "tag": "channel",
      "url-source": "intent://extra/url/url",
      "url-filter": {
        "ignore": ".*(qq|myun\\.tenpay)\\.com/[\\s\\S]*",
        "force": "(http|https)://(.*weixin\\.qq\\.com)/.*"
      }
    },
    {
      "tag": "default",
      "url-source": "intent://extra/url",
      "url-filter": {
        "ignore": ".*((qq|myun\\.tenpay)\\.com|ftn_handler|:8080)/[\\s\\S]*",
        "force": "(http|https)://(.*weixin\\.qq\\.com)/.*"
      }
    }
  ]
}
